{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@dzikimundial-ws/api-interfaces": ["libs/api-interfaces/src/index.ts"],
      "@dzikimundial-ws/assets": ["libs/assets/src/index.ts"],
      "@dzikimundial-ws/pipes/sort-array-by-key": ["libs/pipes/sort-array-by-key/src/index.ts"],
      "@dzikimundial-ws/services/notifications": ["libs/services/notifications/src/index.ts"],
      "@dzikimundial-ws/theme": ["libs/theme/src/index.ts"],
      "@dzikimundial-ws/ui-button": ["libs/ui-button/src/index.ts"],
      "@dzikimundial-ws/ui-flag": ["libs/ui-flag/src/index.ts"],
      "@dzikimundial-ws/ui-icon-button": ["libs/ui-icon-button/src/index.ts"],
      "@dzikimundial-ws/ui-logo": ["libs/ui-logo/src/index.ts"],
      "@dzikimundial-ws/ui-nav-link": ["libs/ui-nav-link/src/index.ts"],
      "@dzikimundial-ws/ui-tooltip": ["libs/ui-tooltip/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
