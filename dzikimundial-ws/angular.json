{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api-interfaces/**/*.ts"]
          }
        }
      }
    },
    "assets": {
      "projectType": "library",
      "root": "libs/assets",
      "sourceRoot": "libs/assets/src",
      "prefix": "dzikimundial-ws",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/assets"],
          "options": {
            "jestConfig": "libs/assets/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/assets/src/**/*.ts", "libs/assets/src/**/*.html"]
          }
        }
      }
    },
    "dzikimundial-ui": {
      "projectType": "application",
      "root": "apps/dzikimundial-ui",
      "sourceRoot": "apps/dzikimundial-ui/src",
      "prefix": "dzikimundial-ws",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/dzikimundial-ui",
            "index": "apps/dzikimundial-ui/src/index.html",
            "main": "apps/dzikimundial-ui/src/main.ts",
            "polyfills": "apps/dzikimundial-ui/src/polyfills.ts",
            "tsConfig": "apps/dzikimundial-ui/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/dzikimundial-ui/src/favicon.ico",
              "apps/dzikimundial-ui/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/assets/src/lib",
                "output": "./assets"
              }
            ],
            "styles": [
              "apps/dzikimundial-ui/src/styles.scss",
              "./libs/theme/src/lib/theme.scss",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/dzikimundial-ui/src/environments/environment.ts",
                  "with": "apps/dzikimundial-ui/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "dzikimundial-ui:build:production"
            },
            "development": {
              "browserTarget": "dzikimundial-ui:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/dzikimundial-ui/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dzikimundial-ui:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/dzikimundial-ui/src/**/*.ts", "apps/dzikimundial-ui/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dzikimundial-ui"],
          "options": {
            "jestConfig": "apps/dzikimundial-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "apps/dzikimundial-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/dzikimundial-ui",
            "config": {
              "configFolder": "apps/dzikimundial-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "dzikimundial-ui-e2e": {
      "root": "apps/dzikimundial-ui-e2e",
      "sourceRoot": "apps/dzikimundial-ui-e2e/src",
      "projectType": "application",
